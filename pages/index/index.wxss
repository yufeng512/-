.warpper{
  position: relative;
  width: 100vw;
}
.warpper .img-box{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100vw;
}
.warpper .content{
  position: absolute;
  width: 100vw;
  padding: 0 20vw;
  top: 960rpx;
  z-index: 2;
}
.item{
  height: 70rpx;
  margin: 30rpx 4vw;
  padding: 10rpx 0; 
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,) 
}
.item .item-img{
  width: 60rpx;
  height: 60rpx;
  margin: 0 10rpx;
  flex: 0 1 auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.item .item-img image{
  width: 36rpx;
}
.item .select-box{
  flex: 1 1 auto;
  font-size: 24rpx;
  text-align: center;
  color: #333333;
}
.item .select-box picker{
  height: 70rpx;
  line-height: 70rpx;
  font-weight: bold
}