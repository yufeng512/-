/* pages/orederInfo/orederInfo.wxss */
.warpper{
  width: 100vw;
  height: 100vh;
}
.container{
  position: relative;
  width: 100vw;
  height: 100vh;
}
.warpper .img-box{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100vw;
}
.code-mask{
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.7);
  z-index: 6;
}
.code-mask .input-code{
  position: absolute;
  left: 20%;
  top: 25vh;
  width: 60vw;
  z-index: 7;
  padding: 30rpx 10rpx;
  border-radius: 10rpx;
  background: #ffffff;
  border: 10rpx solid #333333; 
  box-shadow: -3px -3px 6px #eea0ae inset;
}
.code-mask .input-code input{
  margin:40rpx 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  word-spacing: 20rpx;
  letter-spacing: 20rpx;
  border-bottom: 1px solid #dbdbdb;
}
.code-mask .input-code .text{
  text-align: center;
  font-size: 32rpx;
  font-weight: bolder;
  color: #000000;
}
.code-mask .input-code button{
  width: 40vw;
  height: 72rpx;
  line-height: 72rpx;
}
.warpper .content{
  position: absolute;
  top: 140rpx;
  z-index: 2;
  width: 100vw;
  padding: 0 13vw;
}
.tip {
  text-align: center;
  background: #eea0ae;
}
.select-tip{
  text-align: center;
  color: #999999;
  font-size: 26rpx;
}
.tip text{
  color: #ffffff;
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.warpper .mask{
  position: absolute;
  top: 140rpx;
  width: 100vw;
  padding: 0 13vw;
  z-index: 2;
}
.item{
  /* border: 1rpx dashed rgba(254,105,127,0.4); */
  height: 90rpx;
  margin: 30rpx 0;
  padding: 10rpx 0; 
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,) 
}
.item .item-btn{
  flex: 1 1 auto;
  text-align: center;
  color: #fe697f;
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.item .item-img{
  width: 8vw;
  height: 60rpx;
  margin: 0 10rpx;
  flex: 0 1 auto;
  display: flex;
  align-items: center;
  justify-content: center;
}
.item .select-box{
  flex: 1 0 auto;
  font-size: 24rpx;
  text-align: left;
  color: #666666;
  margin-right: 2vw;
}
.mask .item .select-box picker{
  height: 82rpx;
  line-height: 82rpx;
}
.content .item .select-box picker{
  width: 60vw;
  height: 82rpx;
  line-height: 82rpx;
}
.item .select-box .picker{
  height: 82rpx;
  line-height: 82rpx;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  text-indent: 20rpx;
}
.item .item-img image{
  width: 36rpx;
}
.item .input-box{
  flex: 1 1 auto;
}
.item .input-box input{
  font-size: 28rpx;
  text-align: left;
  color: #666666;
  padding-left: 20rpx; 
}
.check-box text{
  font-size: 28rpx;
  color: #666666;
  border-bottom: 1px solid #999999; 
}
.btn-box{
  margin: 80rpx 0 60rpx;
}
.comfirn-btn{
  margin: 40rpx 0 60rpx;
}
.comfirn-btn button,
.btn-box button{
  width: 60vw;
}
.code-mask .input-code button,
.comfirn-btn button,
.btn-box button{
  color: #333333;
  font-weight: bolder;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,)
}
.time-box .time-item{
  height: 80rpx;
  justify-content:center;
  font-size: 32rpx;
}
.time-box .time-item .item-bg {
  width: 70vw;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.time-box .time-item .item-bg .border{
  display: block;
  width: 30rpx;
  height: 30rpx;
  border: 6rpx solid #ff697f;
  border-radius: 50%;
  background: #ffced0;
  margin-right: 60rpx;
}
.time-box .time-item text{
  font-size: 28rpx;
}
.time-box .time-item.active{
  color: #fe697f;
}
.time-box .time-item .item-bg.current-bg {
  background:rgba(254,105,127,0.2);
}
.time-box .time-item .item-bg.current-bg text{
  color:rgba(254,105,127,1);
}
.week-box,.day-box{
  margin: 20rpx 0;
  padding: 0 30rpx;
}
.week-box .day-item{
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
}
.day-box .day-item{
  flex: 1;
  text-align: center;
  color: #fe697f;
  font-size: 28rpx;
}
.day-box .day-item .text{
  border: 1rpx solid #999999;
  color: #666666;
  font-size: 18rpx;
  margin: 10rpx 5rpx;
  padding: 5rpx 0;
  border-radius: 4rpx;
}
.day-box .day-item .text.active{
  border: 1rpx solid #fe697f;
  background: #fe697f;
  color: #ffffff;
}
.item .item-bg{
  flex: 1 1 auto;
  width: 120rpx;
  margin-right: 20rpx;
  text-align: center;
  color: #333333;
  font-size: 26rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,)
}
